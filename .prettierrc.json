{
  "singleQuote": true,
  "semi": false,
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "jsxSingleQuote": true,
  "trailingComma": "none",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-svelte"],
  "svelteSortOrder": "options-scripts-markup-styles",
  "svelteStrictMode": true,
  "svelteAllowShorthand": true
}
